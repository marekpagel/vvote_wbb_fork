{
	"type":"object",
	"id": "#",
	"required":["boothID","boothSig","serialNo","serialSig","permutation","commitWitness","type","district"],
	"additionalProperties":true,
	"properties":{
		"boothID": {
			"type":"string",
			"id": "boothID"
		},
		"boothSig": {
			"type":"string",
			"id": "boothSig",
			"pattern":"^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
		},
		"serialNo": {
			"type":"string",
			"id": "serialNo",
			"pattern" : "^[A-Za-z0-9]*:[0-9]{1,7}$"
		},
		"serialSig": {
			"type":["string","array"],
			"id": "serialSig",
			"pattern":"^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$",
			"items":{
					"type":"object",
					"id": "serialSigs",
					"required":["WBBID","WBBSig"],
					"additionalProperties":true,
					"properties": { 
						"WBBID": {
							"type":"string",
							"id": "WBBID"
						},
						"WBBSig": {
							"type":"string",
							"id": "WBBSig",
							"pattern":"^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
						}
					}
				}
		
					
		},
		"permutation": {
			"type":"string",
			"id": "permutation",
			"pattern":"^([0-9]+(,[0-9]+)*:[0-9]+(,[0-9]+)*:[0-9]+(,[0-9]+)*:)$"
		},
		"commitWitness": {
			"type":"string",
			"id": "commitWitness",
			"pattern":"^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/];{2}==|[A-Za-z0-9+/]{3}=)?$"
		},
		"type": {
			"type":"string",
			"id": "type",
			"enum": ["audit"]
		},"district": {
			"type":"string",
			"id": "district"
		}
	}
}